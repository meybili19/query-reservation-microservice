generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_RESERVATIONS_DSN")
}

model Reservations {
  id            Int      @id @default(autoincrement())
  user_id       Int
  car_id    Int
  parking_lot_id Int
  start_date    DateTime
  end_date      DateTime
  status        String
  total_amount  Float
}
